@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<Web.ResourceModels.NoteModel>

<div class="text-center">
    <table>
        @foreach (var data in Model)
        {
            <tr>
                <td>
                    @data.title
                </td>
                <td>
                    @data.text
                </td>
                <td>
                    @data.CreateDate
                </td>
            </tr>
        }
    </table>
</div>
